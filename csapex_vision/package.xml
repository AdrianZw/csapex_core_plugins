<?xml version="1.0"?>
<package>
  <name>csapex_vision</name>
  <version>0.0.2</version>
  <description>The csapex_vision package provides a gui for evaluation vision algorithms</description>

  <maintainer email="sebastian.buck@uni-tuebingen.de">buck</maintainer>

  <license>BSD</license>
  <author email="sebastian.buck@uni-tuebingen.de">buck</author>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>utils_qt</build_depend>
  <build_depend>utils_vision</build_depend>
  <build_depend>utils_vision</build_depend>
  <build_depend>csapex</build_depend>
  <build_depend>csapex_core_plugins</build_depend>
  <build_depend>cv_bridge</build_depend>

  <run_depend>utils_qt</run_depend>
  <run_depend>utils_vision</run_depend>
  <run_depend>utils_vision</run_depend>
  <run_depend>csapex</run_depend>
  <run_depend>csapex_core_plugins</run_depend>
  <run_depend>cv_bridge</run_depend>

  <export>
    <csapex plugin="${prefix}/plugins.xml" />
  </export>
</package>
