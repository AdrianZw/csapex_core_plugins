<?xml version="1.0"?>
<package>
  <name>vision_plugins</name>
  <version>0.0.0</version>
  <description>The image_transformation package contains image transformation plugins for the vision evaluator.</description>
  <maintainer email="richard.hanten@uni-tuebingen.de">hanten</maintainer>
 <license>BSD</license>

 <buildtool_depend>catkin</buildtool_depend>

 <build_depend>vision_evaluator</build_depend>
 <build_depend>utils</build_depend>
 <build_depend>roscpp</build_depend>

 <run_depend>vision_evaluator</run_depend>

 <export>
   <vision_evaluator plugin="${prefix}/plugins.xml" />
 </export>
</package>
