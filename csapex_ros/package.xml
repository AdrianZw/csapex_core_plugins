<?xml version="1.0"?>
<package>
  <name>csapex_ros</name>
  <version>0.0.0</version>
  <description>The csapex_ros package</description>

  <maintainer email="sebastian.buck@uni-tuebingen.de">buck</maintainer>
  <license>BSD</license>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>roscpp</build_depend>
  <build_depend>rosapi</build_depend>
  <build_depend>csapex</build_depend>
  <build_depend>csapex_core_plugins</build_depend>
  <build_depend>rosbag</build_depend>
  <build_depend>tf</build_depend>
  <build_depend>pluginlib</build_depend>

  <run_depend>roscpp</run_depend>
  <run_depend>rosapi</run_depend>
  <run_depend>csapex</run_depend>
  <run_depend>csapex_core_plugins</run_depend>
  <run_depend>rosbag</run_depend>
  <run_depend>tf</run_depend>
  <run_depend>pluginlib</run_depend>

  <export>
    <csapex plugin="${prefix}/plugins.xml" />
  </export>
</package>
